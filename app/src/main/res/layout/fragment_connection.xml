<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".views.activities.MainActivity$PlaceholderFragment">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Phone Type:"
        android:id="@+id/cData1"
        android:layout_below="@+id/val_cProvider"
        android:layout_alignParentStart="true"
        android:layout_marginTop="15dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="-"
        android:id="@+id/val_cProvider"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/val_cData1" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Network Country Code:"
        android:id="@+id/cData2"
        android:layout_alignTop="@+id/val_cData2"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="-"
        android:id="@+id/val_cData3"
        android:layout_marginTop="16dp"
        android:layout_below="@+id/cData2"
        android:layout_alignStart="@+id/val_cData2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="-"
        android:id="@+id/val_cData4"
        android:layout_marginStart="23dp"
        android:layout_alignTop="@+id/cData4"
        android:layout_toEndOf="@+id/cData2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Network Type:"
        android:id="@+id/cData3"
        android:layout_alignTop="@+id/val_cData3"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Signal Strength:"
        android:id="@+id/cData4"
        android:layout_below="@+id/cData3"
        android:layout_alignParentStart="true"
        android:layout_marginTop="17dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="-"
        android:id="@+id/val_cData1"
        android:layout_alignTop="@+id/cData1"
        android:layout_alignStart="@+id/val_cData2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="-"
        android:id="@+id/val_cData2"
        android:layout_below="@+id/val_cData1"
        android:layout_alignStart="@+id/val_cData4"
        android:layout_marginTop="17dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Available Base Stations:"
        android:id="@+id/cData6"
        android:layout_below="@+id/cData5"
        android:layout_alignParentStart="true"
        android:layout_marginTop="16dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="-"
        android:id="@+id/val_cData6"
        android:layout_alignTop="@+id/cData6"
        android:layout_alignStart="@+id/val_cData4" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Registered Base Stations:"
        android:id="@+id/cData7"
        android:layout_below="@+id/cData6"
        android:layout_alignParentStart="true"
        android:layout_marginTop="16dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="-"
        android:id="@+id/val_cData7"
        android:layout_alignTop="@+id/cData7"
        android:layout_alignStart="@+id/val_cData6" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Signal Quality:"
        android:id="@+id/cData5"
        android:layout_below="@+id/cData4"
        android:layout_alignParentStart="true"
        android:layout_marginTop="17dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="-"
        android:id="@+id/val_cData5"
        android:layout_alignTop="@+id/cData5"
        android:layout_alignStart="@+id/val_cData7" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Operator:"
        android:id="@+id/cProvider"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/refreshIcon"
        android:src="@mipmap/refresh"
        android:layout_below="@+id/val_cData7"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Parameter_1"
        android:id="@+id/cData8"
        android:layout_marginTop="52dp"
        android:layout_below="@+id/cData7"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Parameter_2"
        android:id="@+id/cData9"
        android:layout_below="@+id/cData8"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Parameter_3"
        android:id="@+id/cData10"
        android:layout_below="@+id/cData9"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Parameter_4"
        android:id="@+id/cData11"
        android:layout_below="@+id/cData10"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Parameter_5"
        android:id="@+id/cData12"
        android:layout_below="@+id/cData11"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="-"
        android:id="@+id/val_cData8"
        android:layout_alignTop="@+id/cData8"
        android:layout_alignStart="@+id/val_cData7" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="-"
        android:id="@+id/val_cData9"
        android:layout_alignTop="@+id/cData9"
        android:layout_alignStart="@+id/val_cData8" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="-"
        android:id="@+id/val_cData10"
        android:layout_alignTop="@+id/cData10"
        android:layout_alignStart="@+id/val_cData9" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="-"
        android:id="@+id/val_cData11"
        android:layout_below="@+id/val_cData10"
        android:layout_alignStart="@+id/val_cData10" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="-"
        android:id="@+id/val_cData12"
        android:layout_alignTop="@+id/cData12"
        android:layout_alignStart="@+id/val_cData11" />

</RelativeLayout>
